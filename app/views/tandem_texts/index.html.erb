<h1>Listing tandem_texts</h1>

<table>
  <tr>
    <th>Content</th>
  </tr>

<% @tandem_texts.each do |tandem_text| %>
  <tr>
    <td><%=h tandem_text.content %></td>
    <td><%= link_to 'Show', tandem_text %></td>
    <td><%= link_to 'Edit', edit_tandem_text_path(tandem_text) %></td>
    <td><%= link_to 'Destroy', tandem_text, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tandem_text', new_tandem_text_path %>